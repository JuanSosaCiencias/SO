@manual{gnu_uname,
  title     = {uname: Print System Information},
  author    = {Free Software Foundation},
  organization = {GNU Project},
  year      = {2023},
  url       = {https://www.gnu.org/software/coreutils/manual/html_node/uname-invocation.html},
  note      = {La documentación oficial explica que el comando \texttt{uname} consulta la información del sistema a través de la función \texttt{uname()} del kernel, que utiliza la estructura \texttt{utsname} para almacenar datos como el nombre del kernel, el nombre del nodo (hostname), la versión del kernel, la fecha de compilación y la arquitectura del hardware.}
}

@manual{compilado2023,
